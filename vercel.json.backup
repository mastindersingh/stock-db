{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    }
  ],
  "routes": [
    {
      "src": "/user-portfolio",
      "dest": "/user-portfolio"
    },
    {
      "src": "/login",
      "dest": "/login"
    },
    {
      "src": "/register",
      "dest": "/register"
    },
    {
      "src": "/logout",
      "dest": "/logout"
    },
    {
      "src": "/",
      "dest": "/"
    }
  ]
}
